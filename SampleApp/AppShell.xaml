<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SampleApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SampleApp">
    <TabBar>
        <Tab Title="Number Generator">
            <!--Icon="cat.png"-->
            <ShellContent ContentTemplate="{DataTemplate local:NumbgenPage}" />
        </Tab>
        <Tab Title="Phone Translator">
            <ShellContent ContentTemplate="{DataTemplate local:TranslatorPage}" />
        </Tab>
        <Tab Title="Binary Translator">
            <ShellContent ContentTemplate="{DataTemplate local:BinaryPage}" />
        </Tab>
    </TabBar>

    <!--<ShellContent
    Title="Home"
    ContentTemplate="{DataTemplate local:NumbgenPage}"
    Route="NumbgenPage" />-->

</Shell>
